context sensors{
	type KeyType : Integer;
	entity temp {
		key tempId: KeyType;
		tempVal: Integer not null;
		ts: UTCTimestamp not null;
		created: UTCTimestamp not null;
	};

	define view tempNoTimestamp as SELECT from temp {
		tempId as "TempId",
		tempVal as "TempVal"
	} with structured privilege check;
	
};